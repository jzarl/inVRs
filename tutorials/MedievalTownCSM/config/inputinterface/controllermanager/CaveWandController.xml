<?xml version="1.0"?>
<controller>
	<data axes="2" buttons="3" sensors="2"/>
	<device name="TrackdDevice">
		<arguments>
			<arg key="controllerKey" type="int" value="4127"/>
			<arg key="trackerKey" type="int" value="4126"/>
		</arguments>
		<button devIdx="0" contIdx="0"/>
		<button devIdx="1" contIdx="1"/>
		<button devIdx="2" contIdx="2"/>
		<axis devIdx="0" contIdx="0">
			<axisValues min="1" max="-1"/>
		</axis>
		<axis devIdx="1" contIdx="1">
			<axisValues min="1" max="-1"/>
		</axis>
		<sensor devIdx="0" contIdx="0">
			<adjustment>
				<translation x="0" y="0" z="-30"/>
				<scale x="33.3" y="33.3" z="33.3"/>
			</adjustment>
		</sensor>
		<sensor devIdx="1" contIdx="1">
			<adjustment>
				<translation x="0" y="0" z="-30"/>
				<scale x="33.3" y="33.3" z="33.3"/>
			</adjustment>
		</sensor>
	</device>
</controller>
